Nand(a=a[13],b=a[13],out=w13);
Nand(a=b[13],b=b[13],out=x13);
Nand(a=w13,b=x13,out=out[13]);

Nand(a=a[14],b=a[14],out=w14);
Nand(a=b[14],b=b[14],out=x14);
Nand(a=w14,b=x14,out=out[14]);

Nand(a=a[15],b=a[15],out=w15);
Nand(a=b[15],b=b[15],out=x15);
Nand(a=w15,b=x15,out=out[15]);
}